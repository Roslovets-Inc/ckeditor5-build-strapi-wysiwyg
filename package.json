{
  "name": "ckeditor5-build-strapi-wysiwyg",
  "version": "1.5.0",
  "description": "CKEditor 5 build for Strapi WYSIWYG editor replacement",
  "private": false,
  "keywords": [
    "ckeditor5-build",
    "ckeditor",
    "ckeditor5",
    "ckeditor 5",
    "wysiwyg",
    "rich text",
    "editor",
    "html",
    "contentEditable",
    "editing",
    "operational transformation",
    "ot",
    "collaboration",
    "collaborative",
    "real-time",
